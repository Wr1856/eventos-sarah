{
  "root": true,
  "parser": "@typescript-eslint/parser",
  "extends": ["eslint:recommended"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "es2021": true,
    "node": true,
    "browser": true
  },
  "rules": {
    "no-unused-vars": "off",
    "no-undef": "off",
    "no-redeclare": "off"
  }
}
